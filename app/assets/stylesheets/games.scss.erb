#chat_window{
  background-image: url("<%= image_path('blue-brick-wall-texture.jpg')%>");
  background-size:100%;
  height:350px;
  border-bottom:5px solid #2a4551;
  border-top:5px solid #2a4551;
  border-left:5px outset #2a4551;
  color:white;
  ul {
    list-style-type:none;
    color:white;
    margin:5px;
    padding:0px;
    div#name{
