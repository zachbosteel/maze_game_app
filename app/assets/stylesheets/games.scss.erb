      -moz-transition: all 200ms cubic-bezier(0.42, 0, 0.58, 1);
      -o-transition: all 200ms cubic-bezier(0.42, 0, 0.58, 1);
      transition: all 200ms cubic-bezier(0.42, 0, 0.58, 1);
    }
    li:hover {
      color:white;
      text-shadow: 0 0 10px rgba(255,255,255,1) , 0 0 20px rgba(255,255,255,1) , 0 0 30px rgba(255,255,255,1) , 0 0 40px #DCE6C8 , 0 0 70px #DCE6C8 , 0 0 80px #DCE6C8 , 0 0 100px #DCE6C8 ;
    }
  }
  button#create{
    color:black;
