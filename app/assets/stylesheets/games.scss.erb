#game_form {
  background-color:#335e70;
  height:350px;
  border-top:5px outset #2a4551;
  border-left:5px outset #2a4551;
  color:white;
  h3{
    display:block;
  }
  div#overlay{
    background-image: url("<%= image_path('mazelogodesaturatedsizeadjusted.png') %>");
    background-repeat: no-repeat;
    background-position:center;
    opacity: 0.6; 
  }
  ul.join-game{
    margin:4px;
    padding:0px;
  }
  ul {
    list-style-type:none;
    color:white;
    li.player{
      margin-top:20px;
      margin-bottom:20px;
      font-size:25px;
    }
    li {
      color:white;
      text-align:center;
      -webkit-box-sizing: border-box;
      -moz-box-sizing: border-box;
      box-sizing: border-box;
